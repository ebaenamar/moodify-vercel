{
  "version": 2,
  "builds": [
    { "src": "frontend/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { 
      "src": "/audio/(.*\\.mp3)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/frontend/public/audio/$1"
    },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
